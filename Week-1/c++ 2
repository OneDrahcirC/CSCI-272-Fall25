#include <iostream>
using namespace std;

void arr(int arr[], int length) {
    for (int i = 0; i < length; i++) {
        cin >> arr[i];
    }
}

int max(int arr[], int length) {
    int maxVal = arr[0];
    for (int i = 1; i < length; i++) {
        if (arr[i] > maxVal) {
            maxVal = arr[i];
        }
    }
    return maxVal;
}

int min(int arr[], int length) {
    int minVal = arr[0];
    for (int i = 1; i < length; i++) {
        if (arr[i] < minVal) {
            minVal = arr[i];
        }
    }
    return minVal;
}

int main() {
    int size = 5;
    int numbers[5];

    arr(numbers, size);
    int maxVal = max(numbers, size);
    int minVal = min(numbers, size);

    cout << "Maximum value: " << maxVal << endl;
    cout << "Minimum value: " << minVal << endl;
}